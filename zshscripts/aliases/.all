#!/bin/bash

alias cls='clear'
alias nv='node -v'
alias conf='code ~/.config/'
alias config='code ~/.zshrc'
alias cdc='cd ~/.config/'
alias reload='source ~/.zshrc'
alias gitconf='code ~/.gitconfig'
alias hla='ls -hla'
alias gi='go install'
alias gb='go build'
alias rmf="rm -rf"
alias bi="bundle install"
alias treload="pbcopy < ~/.config/zshscripts/templates/.tmux.conf && pbpaste > ~/.tmux.conf"
source ~/.config/zshscripts/aliases/.tmux
source ~/.config/zshscripts/aliases/.cargo
source ~/.config/zshscripts/aliases/.git
